
import BSsync.Semaphore;

public class Main {
    public static void main(String[] args) {
        RingBuffer buffer = new RingBuffer(5);
        Semaphore emptySem = new Semaphore(5);
        Semaphore fullSem = new Semaphore(0);
        Producer producer1 = new Producer(buffer, "producer 1", emptySem, fullSem);
        Producer producer2 = new Producer(buffer, "producer 2", emptySem, fullSem);
        Consumer consumer1 = new Consumer(buffer, "consumer 1", emptySem, fullSem);
        Consumer consumer2 = new Consumer(buffer, "consumer 2", emptySem, fullSem);

        producer1.start();
        producer2.start();
        consumer1.start();
        consumer2.start();
        System.out.println("End of main thread.");
    }
}